mixin cards(titleName,menu=false,classMenu)
  // cards
  .cards
    if titleName
      h2.title= titleName

    if menu == true

      div(class= classMenu)
        .wrapper
          ul#tabs_triggers.tabs_triggers
            li(data-trigger="0").js-tabs-trigger.active
              a(href="#") Все категории
            li(data-trigger="1").js-tabs-trigger
              a(href="#") Клубничные букеты
            li(data-trigger="2").js-tabs-trigger
              a(href="#") Клубничные боксы
            li(data-trigger="3").js-tabs-trigger
              a(href="#") Воздушные боксы
            li(data-trigger="4").js-tabs-trigger
              a(href="#") Хит Букеты
            li(data-trigger="5").js-tabs-trigger
              a(href="#") Букеты
            li(data-trigger="6").js-tabs-trigger
              a(href="#") Букеты клубничные
            li.tabsDrop
              span.tabsDrop_span.mobile-hide Показать еще
              span.tabsDrop_span.mobile-visible Показать все категории
              img(src="img/arrow.svg", alt="arrow")
              ul#tabsDrop_list.tabsDrop_list

    .wrapper
      .row

        .tabs_col
          .card
            .card_slider
              .js-card-slider
                a(href="#").card_img.img-responsive
                  img(data-lazy="img/flower_1.jpg", alt="")
                a(href="#").card_img.img-responsive
                  img(data-lazy="img/flower_1.jpg", alt="")
              .card_arrow.card_arrow--prev
                img(src="img/arrow.svg", alt="")
              .card_arrow.card_arrow--next
                img(src="img/arrow.svg", alt="")
            form(action="").card_content
              a(href="#").card_name Тиффани M
              .card_sizes
                input(type="radio" name="flower_1").card_input#flower_1_0
                label(for="flower_1_0").card_label xs
                input(type="radio" name="flower_1").card_input#flower_1_1
                label(for="flower_1_1").card_label s
                input(type="radio" name="flower_1" checked).card_input#flower_1_2
                label(for="flower_1_2").card_label m
                input(type="radio" name="flower_1").card_input#flower_1_3
                label(for="flower_1_3").card_label l
                input(type="radio" name="flower_1").card_input#flower_1_4
                label(for="flower_1_4").card_label xl
              .card_prices
                .card_price.rank 5329₽
              button(type="submit").card_btn.btn-transparent в корзину
              .card_more
                a(href="#") Подробнее
            .card_chips
